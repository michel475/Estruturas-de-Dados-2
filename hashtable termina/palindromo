#include <stdio.h>
#include <string.h>
#include <ctype.h>
#include <stdlib.h>
#define TAM 100

int main()
{
	char string[TAM], aux[TAM];
	fflush(stdin);
	scanf("%[^\n]s", string);
	int i, j = strlen(string)-1, k=0;
	for(i=0; i<strlen(string); i++)
	{
		if(string[i] != ' ' && !ispunct(string[i]))
		{
			aux[j] = tolower(string[i]);
			j--;
		}
	}
	for(; j<strlen(string)-1; j++)
	{
		aux[k] = aux[j+1];
		k++;
	}
	aux[k] = '\0';
	k=0;
	int flag=0;
	for(i=0; i<strlen(string) && flag == 0; i++)
	{
		if(string[i] != ' ' && !ispunct(string[i]))
		{
			if(aux[k] != string[i])
				flag=1;
			k++;
		}
	}
	printf("%s\n", aux);
	if(flag == 1)
		printf("diferentes\n");
	else
		printf("inguais\n");
	/*if(strcmp(aux, aux2) == 0)
		printf("eh palindromo\n");
	else
		printf("nao eh palindromo\n");*/
}
